    "from __future__ import division, print_function\n",
    "from math import *\n",
    "from vpython import *\n",
    "import random\n",
    "a = [sphere(pos= vector(-1,0,0)) for i in range(20)]\n",
    "\n",
    "for i in range(2000):\n",
    "    rate(20)\n",
    "    for a_i in a:\n",
    "        a_i.pos += vector(random.random()-0.5,random.random()-0.5,0)"
